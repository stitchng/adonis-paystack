{
  "name": "adonisjs-paystack",
  "version": "0.1.1",
  "description": "An addon/plugin package to provide PayStack payment services in AdonisJS 4.1+",
  "main": "index.js",
  "files":[
    "src",
    "config",
    "providers",
    "instructions.js",
    "instructions.md",
    "index.js"
  ],
  "directories":{
    "test":"test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "paystack",
    "payments",
    "API",
    "adonisjs",
    "adonis-framework"
  ],
  "author": "StitchNG <stitchnig@gmail.com> (https://twitter.com/stitchappn)",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "standard": {
    "globals": [
      "use"
    ]
  },
  "dependencies": {
    "@adonisjs/generic-exceptions":"^2.0.1",
    "paystack":"^0.1.0",
    "debug":"^3.1.2"
  },
  "devDependencies": {
    "japa": "^2.0.6",
    "japa-cli":"^2.0.0",
    "nyc": "^13.0.1",
    "semver":"^5.5.0",
    "coveralls": "^3.0.2",
    "standard": "^12.0.1",
    "@adonisjs/sink":"^1.0.6",
    "@adonisjs/fold": "^4.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stitchng/adonis-paystack.git"
  },
  "bugs": {
    "url": "https://github.com/stitchng/adonis-paystack/issues"
  },
  "homepage": "https://github.com/stitchng/adonis-paystack#readme"
}
